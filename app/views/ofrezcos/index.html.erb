<p id="notice"><%= notice %></p>

<h1>Ofrezcos</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Start</th>
      <th>Finish</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ofrezcos.each do |ofrezco| %>
      <tr>
        <td><%= ofrezco.course %></td>
        <td><%= ofrezco.start %></td>
        <td><%= ofrezco.finish %></td>
        <td><%= ofrezco.user_id %></td>
        <td><%= link_to 'Show', ofrezco %></td>
        <td><%= link_to 'Edit', edit_ofrezco_path(ofrezco) %></td>
        <td><%= link_to 'Destroy', ofrezco, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ofrezco', new_ofrezco_path %>
