<p id="notice"><%= notice %></p>

<h1>Relacion Buscos</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Busco</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @relacion_buscos.each do |relacion_busco| %>
      <tr>
        <td><%= relacion_busco.user_id %></td>
        <td><%= relacion_busco.busco_id %></td>
        <td><%= link_to 'Show', relacion_busco %></td>
        <td><%= link_to 'Edit', edit_relacion_busco_path(relacion_busco) %></td>
        <td><%= link_to 'Destroy', relacion_busco, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Relacion Busco', new_relacion_busco_path %>
