<p id="notice"><%= notice %></p>

<h1>Buscos</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Start</th>
      <th>Finish</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @buscos.each do |busco| %>
      <tr>
        <td><%= busco.course %></td>
        <td><%= busco.start %></td>
        <td><%= busco.finish %></td>
        <td><%= link_to 'Show', busco %></td>
        <td><%= link_to 'Edit', edit_busco_path(busco) %></td>
        <td><%= link_to 'Destroy', busco, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Busco', new_busco_path %>
