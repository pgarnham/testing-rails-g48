<p id="notice"><%= notice %></p>

<h1>Sol Moderadors</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sol_moderadors.each do |sol_moderador| %>
      <tr>
        <td><%= sol_moderador.user_id %></td>
        <td><%= link_to 'Show', sol_moderador %></td>
        <td><%= link_to 'Edit', edit_sol_moderador_path(sol_moderador) %></td>
        <td><%= link_to 'Destroy', sol_moderador, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sol Moderador', new_sol_moderador_path %>
